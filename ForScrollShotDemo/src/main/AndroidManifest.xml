<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.tplink.forscrollshotdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".GridActivity">
        </activity>
        <activity android:name=".ListActivity">
        </activity>
        <activity android:name=".ScrollActivity">
        </activity>
        <activity android:name=".GridWithHeaderActivity">
        </activity>
        <activity android:name=".ListWithHeaderActivity">
        </activity>
        <activity android:name=".ScrollWithHeaderActivity">
        </activity>
    </application>

</manifest>